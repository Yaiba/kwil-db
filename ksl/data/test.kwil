
table users {
    id:    int64     @id
    email: string    @unique
    name:  string? @index("users_name_idx")
    dob:   date?
    aliases: string[]
    created_at: datetime
    updated_at: datetime
}

table posts {
    id:        int64     @id
    title:     string?   @size(256) @unique @index("posts_title_idx")
    content:   string?
    e_test: test_enum    @default(one)
    published: bool      @default(true)
    author_id: int64     @foreign_key(users.id, on_delete="SET NULL")
}

enum test_enum {
    one
    two
    three
    four
}
